{
  "name": "moonlight-plugins",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node build.mjs",
    "dev": "node build.mjs --watch",
    "clean": "node build.mjs --clean",
    "repo": "node repo.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:report": "eslint --output-file eslint_report.json --format json src",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck"
  },
  "devDependencies": {
    "@electron/asar": "^3.4.1",
    "@moonlight-mod/esbuild-config": "^1.0.0",
    "@moonlight-mod/eslint-config": "github:moonlight-mod/eslint-config",
    "esbuild": "^0.19.12",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@moonlight-mod/types": "^1.3.27"
  }
}